@import "../../component/common/mixin.scss";
.ant-popover-content {
    .ant-popover-inner-content {
        padding: 0;
    }
    .class-list {
        width: 184px;
        padding: 0;
        margin: 0;
        list-style: none;
        background-color: #1C474F;
        li {
            padding-left: 14px;
            line-height: 26px;
            font-size: 14px;
            color: #75eee5;
            cursor: pointer;
            @include textOverFlow;
            &:hover {
                background-color: #265B64;
            }
        }
    }
}

.lg-PersonalDetail {
    position: relative;
    height: 100%;
    width: 100%;
    overflow-y: auto;
    overflow-x: hidden;
    .control-rate {
        position: absolute;
        top: 25px;
        right: 50px;
        width: 108px;
        height: 54px;
        background-color: rgba($color: #ffffff, $alpha: 0.2);
        border-radius: 5px;
        z-index: 10;
        overflow: hidden;
        cursor: pointer;
        opacity: 0.95;
        &:hover {
            opacity: 1;
        }
        &::before {
            content: "";
            position: absolute;
            width: 50%;
            height: 100%;
            border-radius: 5px;
            vertical-align: top;
            background-color: rgba($color: #ffffff, $alpha: 0.3);
            left: 0;
            top: 0;
            transition: left 0.1s linear;
            -webkit-transition: left 0.1s linear;
            -moz-transition: left 0.1s linear;
            -ms-transition: left 0.1s linear;
            -o-transition: left 0.1s linear;
        }
        &.control-rate-full {
            &::before {
                left: 0;
            }
        }
        &.control-rate-ratio {
            &::before {
                left: 50%;
            }
        }
        // opacity: 0.2;
        i {
            display: inline-block;
            width: 50%;
            height: 100%;
            border-radius: 5px;
            vertical-align: top;
            // &.ct-select {
            //     background-color: rgba($color: #ffffff, $alpha: 0.3);
            // }
        }
        .ct-full {
            background: url(./images/icon-full.png) no-repeat center center/26px 26px;
            &.ct-select {
                background: url(./images/icon-full-select.png) no-repeat center center/26px 26px;
            }
        }
        .ct-ratio {
            background: url(./images/icon-ratio.png) no-repeat center center/23px 15px;
            &.ct-select {
                background: url(./images/icon-ratio-select.png) no-repeat center center/23px 15px;
            }
        }
    }
    .pd-content {
        position: relative;
        height: 100%;
        width: 100%;
        background: url(./images/image-BG.png) no-repeat top center / 100% 100%;
        // background: url(./images/个人画像.png) no-repeat top center / 100% 100%;
    }
    .pd-top {
        position: relative;
        width: 100%;
        height: 85px;
        .pd-top-bg {
            position: relative;
            width: 100%;
            height: 343px;
            background: url(./images/image-top.png) no-repeat center top / 100% 100%;
        }
    }
    .pd-center {
        position: relative;
        width: 100%;
        height: 995px;
        .pd-center-bg {
            position: absolute;
            bottom: 0;
            left: 50%;
            transform: translateX(-50%);
            width: 1874px;
            height: 930px;
            -webkit-transform: translateX(-50%);
            -moz-transform: translateX(-50%);
