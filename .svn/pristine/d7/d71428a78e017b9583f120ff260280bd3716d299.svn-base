.card-history {
    padding: 50px 48px 0;
    .ch-scrollbars {
        &>div {
            &:nth-of-type(2) {
                &>div {
                    background-color: rgba(256, 256, 256, 0.2) !important;
                }
            }
        }
    }
    .ch-dot {
        position: relative;
        display: inline-block;
        width: 14px;
        height: 14px;
        margin: 0 7px;
        vertical-align: middle;
        background-color: #146289;
        border-radius: 50%;
        -webkit-border-radius: 50%;
        -moz-border-radius: 50%;
        -ms-border-radius: 50%;
        -o-border-radius: 50%;
    }
    .ch-dot-box {
        line-height: 215px;
        white-space: nowrap;
    }
    .ch-dot-active {
        width: 18px;
        height: 18px;
        .ch-dot-polo {
            position: absolute;
            width: 2px;
            height: 101px;
            background-color: #609beb;
            &::before {
                content: "";
                position: absolute;
                width: 30px;
                height: 30px;
                border-radius: 50%;
                -webkit-border-radius: 50%;
                -moz-border-radius: 50%;
                -ms-border-radius: 50%;
                -o-border-radius: 50%;
                border: 3px solid #609beb;
            }
            &::after {
                content: "";
                position: absolute;
                width: 6px;
                left: -2px;
                height: 6px;
                border-radius: 50%;
                background: #609beb;
                -webkit-border-radius: 50%;
                -moz-border-radius: 50%;
                -ms-border-radius: 50%;
                -o-border-radius: 50%;
                // border: 3px solid #609beb;
            }
            .ch-content {
                position: absolute;
                top: 10px;
                left: 2px;
                min-width: 80px;
                max-width: 196px;
                padding: 7px 14px 0;
                height: 56px;
                line-height: 56px;
                border-radius: 1px;
                background: rgba($color: #609beb, $alpha: 0.4);
                &::before {
                    content: "";
                    display: none;
                    position: absolute;
                    top: 0;
                    left: 0;
                    width: 100%;
                    height: 100%;
                    background-color: #609beb;
                    opacity: 0.4;
                    border-radius: 1px;
                    -webkit-border-radius: 1px;
                    -moz-border-radius: 1px;
                    -ms-border-radius: 1px;
                    -o-border-radius: 1px;
                }
                p {
                    margin-bottom: 0;
                    line-height: 20px;
                    @include textOverFlow;
                }
                .ch-content-time {
                    font-size: 12px;
                    color: #609beb;
                }
                .ch-content-title {
                    font-size: 14px;
                    font-weight: bold;
                    color: #98f2ff;
                }
            }
        }
        &:nth-of-type(2n + 1) {
            .ch-dot-polo {
                top: -105px;
                left: 8px;
                &::before {
                    bottom: -28px;
                    left: -14px;
                }
                &::after {
                    top: 4px;
                    left: -2px;
                }
                .ch-content {
                    top: 12px;
                }
            }
        }
        &:nth-of-type(2n) {
            .ch-dot-polo {
                top: 24px;
                left: 8px;
                &::before {
                    top: -30px;
                    left: -14px;
                }
                &::after {
                    left: -2px;
                    bottom: -4px;
                }
                .ch-content {
                    top: auto;
                    bottom: 4px;
                }
            }
        }
        &:nth-of-type(4n + 1) {
            background-color: #609beb;
            .ch-dot-polo {
                background-color: #609beb;
                &::before {
                    border-color: #609beb;
                }
                &::after {
                    background-color: #609beb;
                }
                .ch-content {
                    &::before {
                        background-color: #609beb;
                    }
                    .ch-content-time {
                        color: #609beb;
                    }
                    .ch-content-title {
                        color: #98f2ff;
                    }
                }
            }
        }
        &:nth-of-type(4n + 2) {
            background-color: #3cc3ae;
            .ch-dot-polo {
                background-color: #3cc3ae;
                &::before {
                    border-color: #3cc3ae;
                }
                &::after {
                    background-color: #3cc3ae;
                }
                .ch-content {
                    background: rgba($color: #3cc3ae, $alpha: 0.4);
                    &::before {
                        background-color: #3cc3ae;
                    }
                    .ch-content-time {
                        color: #3b9486;
                    }
                    .ch-content-title {
                        color: #40d2e1;
                    }
                }
            }
        }
        &:nth-of-type(4n + 3) {
            background-color: #ed7757;
            .ch-dot-polo {
                background-color: #ed7757;
                &::before {
                    border-color: #ed7757;
                }
                &::after {
                    background-color: #ed7757;
                }
                .ch-content {
                    background: rgba($color: #ed7757, $alpha: 0.4);
                    &::before {
                        background-color: #ed7757;
                    }
                    .ch-content-time {
                        color: #a45e4b;
                    }
                    .ch-content-title {
                        color: #ffa187;
                    }
                }
            }
        }
        &:nth-of-type(4n) {
            background-color: #3da84c;
            .ch-dot-polo {
                background-color: #3da84c;
                &::before {
                    border-color: #3da84c;
                }
                &::after {
                    background-color: #3da84c;
                }
                .ch-content {
                    background: rgba($color: #3da84c, $alpha: 0.4);
                    &::before {
                        background-color: #3da84c;
                    }
                    .ch-content-time {
                        color: #488450;
                    }
                    .ch-content-title {
                        color: #64ca72;
                    }
                }
            }
        }
    }
    // &>span {
    //     background-color: #146289;
    //     border-radius: 50%;
    //     -webkit-border-radius: 50%;
    //     -moz-border-radius: 50%;
    //     -ms-border-radius: 50%;
    //     -o-border-radius: 50%;
    // }
}

