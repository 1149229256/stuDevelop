                &:hover {
                    background-image: url(../app/images/搜索关闭【经过】.png);
                }
            }
            .searchlogo {
                position: absolute;
                top: 50%;
                right: 12px;
                transform: translateY(-50%);
                display: block;
                width: 18px;
                height: 18px;
                cursor: pointer;
                background-image: url(../app/images/搜索图标_初始.png);
                background-size: 100% 100%;
                &:hover {
                    background-image: url(../app/images/搜索图标_悬停.png);
                }
            }
        }
    }
    .stu-count {
        color: #999999;
        span {
            color: #ff0000;
        }
    }
    .member-list {
        padding: 0;
        margin: 0 0 0 20px;
        list-style: none;
        li {
            width: 50px;
            display: inline-block;
            vertical-align: middle;
            margin: 0 30px 20px 0;
            cursor: pointer;
            &:hover {
                opacity: 0.75;
            }
            img {
                display: block;
                width: 40px;
                height: 40px;
                border-radius: 50%;
                margin: 0 auto;
            }
            .user-header {
                display: block;
                width: 40px;
                height: 40px;
                border-radius: 50%;
                margin: 0 auto;
                background-image: url(../app/images/header-boy.jpg);
                background-size: 100% 100%;
                &.girl-header {
                    background-image: url(../app/images/header-girl.jpg);
                }
            }
            p {
                font-size: 12px;
                color: #999999;
                text-align: center;
                text-overflow: ellipsis;
                overflow: hidden;
                white-space: nowrap;
            }
        }
    }
}

