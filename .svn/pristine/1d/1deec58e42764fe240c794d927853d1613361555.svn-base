.HistoryModal {
    .ant-modal-body {
        padding: 10px 0;
    }
    .type-label {
        margin: 20px 0;
        line-height: 20px;
        font-size: 12px;
        text-align: center;
        .type-name {
            color: #333333;
        }
        .type-select {
            position: relative;
            height: 20px;
            background-color: #d5e8f8;
            border-radius: 3px;
            padding: 0 8px;
            font-size: 12px;
            cursor: pointer;
            color: #034069;
            margin: 0 6px 0 5px;
            &:first-of-type {
                margin-left: 0;
            }
            &::after {
                content: "";
                position: absolute;
                right: -6px;
                width: 1px;
                height: 20px;
                background: url(./images/icon-devide-line.png) no-repeat center center/contain;
            }
            &:last-of-type {
                &::after {
                    display: none;
                }
            }
            &.type-select-active {
                background-color: #0099ff;
                color: #fff;
                &::before {
                    content: "";
                    position: absolute;
                    bottom: -4px;
                    left: 50%;
                    height: 0;
                    width: 0;
                    border-top: 4px solid #0099ff;
                    border-bottom: none;
                    border-left: 4px solid transparent;
                    border-right: 4px solid transparent;
                    transform: translateX(-50%);
                    -webkit-transform: translateX(-50%);
                    -moz-transform: translateX(-50%);
                    -ms-transform: translateX(-50%);
                    -o-transform: translateX(-50%);
                }
            }
        }
    }
    .hm-echarts {
        height: 360px;
        padding: 0;
        .title-2 {
            color: #fff;
        }
    }
}

