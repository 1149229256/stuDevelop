@import "../../component/common/mixin.scss";
.ant-popover-content {
    .ant-popover-inner-content {
        background-color: white;
        .template-list {
            padding: 0;
            margin: 0;
            list-style: none;
            &>span {
                display: block;
            }
            li {
                height: 30px;
                line-height: 30px;
                padding: 0 15px;
                cursor: pointer;
                &:hover {
                    opacity: 0.75;
                }
            }
        }
        .course-class-list {
            padding: 0;
            margin: 0;
            list-style: none;
            &>span {
                display: block;
            }
            li {
                height: 30px;
                line-height: 30px;
                padding: 0 15px;
                cursor: pointer;
                &:hover {
                    opacity: 0.75;
                }
            }
        }
    }
}

.error-container {
    .ant-tooltip-arrow {
        display: none;
    }
    .ant-tooltip-inner {
        background-color: white;
        border-radius: 3px;
        padding: 10px 30px 10px 16px;
        position: relative;
        left: 50px;
    }
    .error-tips {
        p {
            margin: 0;
            padding: 0;
            color: #999999;
            span {
                color: #333333;
            }
        }
    }
}

.Analysis-Top {
    position: relative;
    &:after {
        content: '';
        display: block;
        clear: both;
    }
    .dropdown_title_span {
        color: #999999;
    }
    .select-container {
        display: inline-block;
        vertical-align: middle;
        margin: 12px 24px 12px 0;
        span {
            display: inline-block;
            line-height: 30px;
            color: #999999;
            margin-right: 8px;
        }
        .select {
            &.current-class {
                .ant-select-selector {
                    width: 130px;
                }
            }
            &.grade-select {
                .ant-select-selector {
                    width: 100px;
                }
            }
            &.class-select {
                .ant-select-selector {
                    width: 100px;
                }
            }
            .ant-select-selector {
                width: 80px;
                height: 30px;
                border-radius: 3px;
                vertical-align: middle;
            }
            .ant-select-selection-item {
                color: #333333;
                padding-right: 0;
            }
            .ant-select-arrow {
                right: 0;
            }
        }
