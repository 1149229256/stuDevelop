.list-one {
    height: 32px;
    line-height: 32px;
    .list-title {
        display: inline-block;
        background-color: #f5feff;
        color: #0f99a6;
        text-align: center;
        border: 1px solid #b7e1e5;
        text-overflow: ellipsis;
        overflow: hidden;
        white-space: nowrap;
    }
    .list-content {
        display: inline-block;
        text-align: center;
        color: #333333;
        text-overflow: ellipsis;
        overflow: hidden;
        white-space: nowrap;
        border: 1px solid #b7e1e5;
    }
}